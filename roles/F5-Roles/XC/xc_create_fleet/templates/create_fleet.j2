{
  "name": "{{ova_builds_var.cluster_name}}-fleet",
  "state": "APPROVED",
  "metadata": {
    "name": "{{ova_builds_var.cluster_name}}-fleet",
    "disable": false
  },
  "spec": {
    "network_connectors": [
      {
        "kind": "network_connector",
        "name": "{{ova_builds_var.cluster_name}}-connector"
      }
    ],
    "outside_virtual_network": [
      {
        "kind": "virtual_network",
        "name": "{{ova_builds_var.cluster_name}}-outside"
      }
    ],
    "inside_virtual_network": [
      {
        "kind": "virtual_network",
        "name": "{{ova_builds_var.cluster_name}}-inside"
      }
    ],
    "interface_list": {
      "interfaces": [
        {
          "name": "{{ova_builds_var.hostname}}-0-eth1",
          "kind": "network_interface"
        },
        {
          "name": "{{ova_builds_var.hostname}}-1-eth1",
          "kind": "network_interface"
        },
        {
          "name": "{{ova_builds_var.hostname}}-2-eth1",
          "kind": "network_interface"
        }
      ]
    }
  }
}